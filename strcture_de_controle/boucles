//Partie 1
//les chiffres supérieurs ou égal à 5 ne sont jamais affichés
#include <stdio.h>
int i = 0;
while (i < 10)
{
    if (i == 5) break ; // utilisation de break , on met fin au programme
    printf("%d\n", i);
    i++;
}

int i =0;
while (i < 10)
{
    if (i>=5) {
        i++;
        continue; //on met fin momentanément à l'éxécution du programme
    }
    printf("%d\n",i);
    i++;
}

//Partie 2
// les chiffres supérieurs ou égale à 5 ne sont pas affichés 

for ( int i = 0; i < 10; i++ ){
        if (i == 5) continue ;
        printf("%d\n", i);
    }

int main()
for (int i =0; i<10; i++){
        if (i>=5) break; //utilisation de break, met fin à l'éxécution du programme
        printf("%d\n",i);

}